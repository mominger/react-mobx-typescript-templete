{
  "name": "aaxpress-web-booking",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@airasia/phoenix-components": "^1.158.16",
    "@airasia/phoenix-widgets": "^1.158.16",
    "@i18n-chain/react": "^2.0.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "antd": "^4.19.2",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "customize-cra": "^1.0.0",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-image-file-resizer": "^0.4.8",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "sass-resources-loader": "^2.2.5",
    "styled-components": "^5.3.5",
    "typescript": "^4.4.2",
    "ua-parser-js": "^1.0.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "dotenv -e env/.env.dev react-app-rewired start",
    "mock": "concurrently --kill-others \"nodemon mocks/index.js\" \"dotenv -e env/.env.mock react-app-rewired start\"",
    "dev": "dotenv -e env/.env.dev react-app-rewired start",
    "stg": "dotenv -e env/.env.stg react-app-rewired start",
    "build": "dotenv -e env/.env.prod react-app-rewired build",
    "test": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage",
    "eject": "react-scripts eject",
    "eslint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,ts,jsx,tsx,less,css}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/webpack-env": "^1.17.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-plugin-transform-require-context": "^0.1.1",
    "concurrently": "^7.2.2",
    "dotenv-cli": "^5.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "lint-staged": "^12.4.2",
    "mockjs": "^1.1.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.16",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.2.0",
    "sass-loader": "^13.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "<rootDir>/src/__tests__/unit/*-{spec,test}.js",
      "<rootDir>/src/__tests__/unit/**/*/*-{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/setup/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup/setup-enzyme.js",
      "<rootDir>/src/__tests__/setup/setup-react.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "html",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "engines": {
    "node": ">=14.17.4"
  }
}